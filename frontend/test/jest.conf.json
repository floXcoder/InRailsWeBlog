{
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
    "<rootDir>/spec/javascripts/mocks/fileMock.js",
    "\\.(css|less|scss)$": "<rootDir>/spec/javascripts/mocks/styleMock.js"
  },
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "modulePaths": [
  ],
  "rootDir": "../..",
  "setupFiles": [
    "<rootDir>/spec/javascripts/setup.js"
  ],
  "testMatch": [
    "<rootDir>/spec/javascripts/**/*_spec.jsx"
  ]
}
